<span class='comment'>/* These macros are used by bits/stdio.h and internal headers.  */</span>
<span class='directive'>#define __getc_unlocked_body(_fp)					\
  (__glibc_unlikely ((_fp)-&gt;_IO_read_ptr &gt;= (_fp)-&gt;_IO_read_end)	\
   ? __uflow (_fp) : *(unsigned char *) (_fp)-&gt;_IO_read_ptr++)</span>

<span class='directive'>#define __putc_unlocked_body(_ch, _fp)					\
  (__glibc_unlikely ((_fp)-&gt;_IO_write_ptr &gt;= (_fp)-&gt;_IO_write_end)	\
   ? __overflow (_fp, (unsigned char) (_ch))				\
   : (unsigned char) (*(_fp)-&gt;_IO_write_ptr++ = (_ch)))</span>

<span class='directive'>#define _IO_EOF_SEEN 0x0010</span>
<span class='directive'>#define __feof_unlocked_body(_fp) (((_fp)-&gt;_flags &amp; _IO_EOF_SEEN) != 0)</span>

<span class='directive'>#define _IO_ERR_SEEN 0x0020</span>
<span class='directive'>#define __ferror_unlocked_body(_fp) (((_fp)-&gt;_flags &amp; _IO_ERR_SEEN) != 0)</span>

